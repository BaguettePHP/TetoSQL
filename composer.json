{
    "name": "zonuexe/tetosql",
    "description": "Simple and secure SQL templating",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "pixiv Inc."
        },
        {
            "name": "USAMI Kenta",
            "email": "tadsan@zonu.me"
        }
    ],
    "keywords": [
        "sql"
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1 || ^1.1",
        "phpunit/phpunit": "^7.5 || ^4.8",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Teto\\SQL\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["tests/polyfill.php"],
        "psr-4": {
            "Teto\\SQL\\": "tests/"
        }
    },
    "suggest": {
        "ext-mysql": "MySQL support"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
